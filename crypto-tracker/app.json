{
  "expo": {
    "name": "cryptX",
    "slug": "cryptoX",
	"privacy":"public",
	"description": "Track crypto prices in real time",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ETH.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/ETH.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
		"package":"crypto.tracker",
      "adaptiveIcon": {
        "foregroundImage": "./assets/ETH.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/ETH.png"
    }
  }
}
